# vim: set filetype=sh :

WRAPPER_FILE=$(find /usr -name "virtualenvwrapper.sh" -print 2>/dev/null)
if [ -z "$WRAPPER_FILE"  ]; then
    echo "virtualenvwrapper not present"
    echo
    echo "install it with:"
    echo "  sudo -H pip install virtualenvwrapper"
else
    source "$WRAPPER_FILE"
fi

source ~/configs/generic/._sh_aliases_lenovo
