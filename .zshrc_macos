# Generic zsh stuff
source ~/configs/zsh/.zshrc

# Generic stuff
source ~/configs/generic/.shrc
source ~/configs/generic/.sh_aliases
# Generic macos stuff
source ~/configs/generic/.shrc_macos
source ~/configs/generic/.sh_aliases_macos
source ~/configs/generic/.shrc_functions_macos

source $(brew --prefix)/Cellar/fzf/$(fzf --version | cut -d ' ' -f 1)/shell/key-bindings.zsh

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
